{
	"env": {
		"node": true,
		"browser": true,
		"es6": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:vue/essential"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module",
		"extends": "standard",
  		"parser": "@babel/eslint-parser",
		"requireConfigFile": false
	},
	"plugins": [
		"vue"
	],
	"rules": {
		"no-console": "off"
	}
}